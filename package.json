  "scripts": {
    "start:dev": "next",
    "build:dev": "next build",
    "build": "next build",
    "postbuild": "next-on-netlify",
    "export": "next export",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "10.0.5",
    "next-on-netlify": "^2.7.2",
